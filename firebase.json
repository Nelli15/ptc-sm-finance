{
  "hosting": {
    "public": "dist/spa",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/receipt",
      "function": "getReceipt"
    },
    {
      "source": "/downloadReceiptsZip",
      "function": "downloadReceiptsZip"
    }]
  }
}
